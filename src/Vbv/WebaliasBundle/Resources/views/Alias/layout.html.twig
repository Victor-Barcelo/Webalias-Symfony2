<!DOCTYPE html>
<html>

<head>
    <meta http-equiv="Content-Type" content="text/html" charset="utf-8"/>
    <title>{% block title %}Web Alias{% endblock %}</title>
    {% block stylesheets %}
        <link rel="stylesheet" href="{{ asset('bundles/webalias/css/vendor/bootstrap.min.css') }}">
        <link rel="stylesheet" href="{{ asset('bundles/webalias/css/vendor/bootstrap-theme.min.css') }}">
        <script src="{{ asset( 'bundles/webalias/js/vendor/modernizr-2.6.2-respond-1.1.0.min.js') }}"></script>
        <link href='http://fonts.googleapis.com/css?family=Lobster' rel='stylesheet' type='text/css'>
        <link href="{{ asset( 'bundles/webalias/css/main.css') }} " type="text/css" rel="stylesheet "/>
    {% endblock %}
</head>

<body>

<header>
    {% include 'VbvWebaliasBundle:Alias:header.html.twig' %}
    {% block header %}{% endblock %}
</header>

<main>
    {% block main %}{% endblock %}
</main>

<footer>
    {% include 'VbvWebaliasBundle:Alias:footer.html.twig' %}
    {% block footer %}{% endblock %}
</footer>

{% block javascripts %}
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>
    <script>window.jQuery || document.write('<script src="{{ asset('bundles/webalias/js/vendor/jquery-1.11.1.min.js') }}"><\/script>')</script>
    <script src="{{ asset('bundles/webalias/js/vendor/bootstrap.min.js') }}"></script>
    <script src="{{ asset('bundles/webalias/js/main.js') }}"></script>
{% endblock %}

</body>

</html>
